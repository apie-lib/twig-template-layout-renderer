{
  "name": "apie/twig-template-layout-renderer",
  "description": "Composer package of the apie library: twig template layout renderer",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
